{
  "name": "popcorn4dinner/policies",
  "description": "A flexible, simple to use and non-opinionated collection library",
  "type": "library",
  "version": "0.1.0",
  "license": "MIT",
  "config": {
    "preferred-install": "dist"
  },
  "repositories": {
    "packagist.org": {
      "type": "composer",
      "url": "https://packagist.org"
    }
  },
  "require": {
    "php": ">=7.1",
    "esky/enum": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~7",
    "fzaninotto/faker": "^1.5.0"
  },
  "autoload": {
    "psr-4": {
      "Popcorn4dinner\\Policies\\": "src/"
    }
  },
    "autoload-dev": {
      "psr-4": {
        "Popcorn4dinner\\Policies\\Examples\\": "examples/",
        "Popcorn4dinner\\Policies\\Tests\\": "tests/unit/"
      }
    }
}
